          <Route exact path=\"/AdminDashboard/Question\" component={Question}></Route>\r\n                  <Route exact path=\"/AdminDashboard/Result\" component={Result}></Route>\r\n                  <Route exact path=\"/AdminDashboard/StudentList\" component={StudentList}></Route>\r\n\r\n\r\n                      <Route exact path=\"/AdminDashboard/Exam/Details/:id\" component={Details}></Route>\r\n                      <Route exact path=\"/AdminDashboard/Exam/ViewQuestion/:id\" \r\n                      component={ViewQuestion}></Route>\r\n                      <Route exact path=\"/AdminDashboard/Exam/AddQuestion/:id\" component={AddQuestion}></Route>\r\n\r\n                      <Route exact path=\"/AdminDashboard/StudentList/Details/:id\" component={Student}></Route>\r\n\r\n\r\n              </Switch>\r\n                    \r\n                    </div> \r\n\r\n                </div>\r\n\r\n\r\n                          \r\n                </BrowserRouter>\r\n\r\n              \r\n               \r\n\r\n              \r\n            </>\r\n        );\r\n    }\r\n\r\n    export default AdminDashboard;","C:\\Users\\jaswanth\\Desktop\\spe-major-project\\2-ReactSpringBootApplication-master\\online exam system frontend\\frontend\\src\\Components\\AdminComponent\\AdminLogin\\AdminLogin.js",[],"C:\\Users\\jaswanth\\Desktop\\spe-major-project\\2-ReactSpringBootApplication-master\\online exam system frontend\\frontend\\src\\Components\\StudentComponent\\StudentDashboard\\ResultComponent\\Result.js",[],"C:\\Users\\jaswanth\\Desktop\\spe-major-project\\2-ReactSpringBootApplication-master\\online exam system frontend\\frontend\\src\\Components\\StudentComponent\\StudentDashboard\\ExamComponent\\Exam.js",[],"C:\\Users\\jaswanth\\Desktop\\spe-major-project\\2-ReactSpringBootApplication-master\\online exam system frontend\\frontend\\src\\Components\\StudentComponent\\StudentDashboard\\Subject\\Subject.js",[],"C:\\Users\\jaswanth\\Desktop\\spe-major-project\\2-ReactSpringBoo